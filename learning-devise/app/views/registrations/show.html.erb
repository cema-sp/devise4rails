<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h2><%= @show_user.username %></h2>   
      <div class='form-group'>
        <%= label_tag 'Email', 'Email', class: 'control-label' %><br />
        <%= email_field_tag 'Email', @show_user.email, autofocus: true, class: 'form-control', disabled: true  %>
      </div>

      <div class='form-group'>
        <%= label_tag 'Address', 'Address', class: 'control-label' %><br />
        <%= text_field_tag 'Address', @show_user.address, class: 'form-control', disabled: true  %>
      </div>
    <%= link_to "Back", :back %>
  </div>
</div>